   "source": [
    "def solution(s):\n",
    "    answer = []\n",
    "    for i in range(len(s)):\n",
    "        if s[i] not in s[:i]:\n",
    "            answer.append(-1)\n",
    "        else:\n",
    "            answer.append(i - s[:i].rfind(s[i]))\n",
    "\n",
    "    return answer"
   ]
